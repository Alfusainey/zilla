{
    "name": "test",
    "bindings":
    {
        "net0":
        {
            "type" : "http",
            "kind": "server",
            "options":
            {
                "versions": [ "http/1.1" ],
                "access-control":
                {
                    "policy": "cross-origin",
                    "allow":
                    {
                        "methods": [ "HEAD", "GET", "PUT", "POST", "PATCH", "DELETE" ],
                        "headers": [ "x-api-key" ]
                    }
                }
            },
            "routes":
            [
                {
                    "exit": "app0",
                    "when":
                    [
                        {
                            "headers":
                            {
                                ":scheme": "https",
                                ":authority": "example.com:9090"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
