{
    "bindings":
    [
        {
            "entry": "app#0",
            "type" : "http",
            "kind": "client",
            "options":
            {
                "versions": [ "h2" ]
            },
            "routes":
            [
                {
                    "exit": "net#0",
                    "when":
                    [
                        {
                            "headers":
                            {
                                ":authority": "localhost:8080"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}
